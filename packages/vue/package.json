  "dependencies": {
    "@ldesign/chart-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "echarts": "^5.4.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@vue/compiler-sfc": "^3.4.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
