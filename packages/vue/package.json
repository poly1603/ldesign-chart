{
  "name": "@ldesign/chart-vue",
  "version": "2.0.0",
  "description": "企业级图表 Vue 3 适配器",
  "keywords": [
    "chart",
    "vue",
    "vue3",
    "echarts",
    "visualization"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/chart#readme",
  "repository": {
    "type": "git",
